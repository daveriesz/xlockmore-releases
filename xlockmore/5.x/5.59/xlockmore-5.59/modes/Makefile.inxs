	find $(srcdir) -name '*.[chly]' -print | xargs etags -a

echo_tarfiles:
	@echo $(TARFILES)

validate_xml:
	@echo "Validating XML..." ; \
	cd $(srcdir) ; ./check-configs.pl $(EXES)

