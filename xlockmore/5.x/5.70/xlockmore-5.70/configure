            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $vers" >&5
printf "%s\n" "$vers" >&6; }
            pkgs="$pkgs $req"
            return 1
          else
            { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $vers (wanted >= $min)" >&5
printf "%s\n" "$vers (wanted >= $min)" >&6; }
