Popup "Special-Saver"
	Title	"Special-Saver"
	Exec	"Blank"	exec xlock -nolock -nice 0 -mode blank &
	Exec	"Message"	exec xlock -nolock -nice 0 -mode marquee -message "No one here, please go away!" &
	Exec	"Random"	exec xlock -nolock -nice 0 -mode random &
EndPopup

Popup "Screensaver"
	Title	"Screensaver"
	Popup	"Cellular Automata"	Automata-Saver
	Nop	""
	Popup	"Fractal"	Fractal-Saver
	Nop	""
	Popup	"Geometry"	Geometry-Saver
	Nop	""
	Popup	"Space"	Space-Saver
	Nop	""
	Popup	"Assorted"	Assorted-Saver
	Nop	""
	Popup	"Special"	Special-Saver
EndPopup

